#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double s = 0.0;
    s += 2.0;
    for(int i=1; i<=4 ; i++) {
        double helper = 0.0;
        for(int j=1; j<=4; j++) {
            helper += 1/((j*j) + sqrt(5));
        }
        s += sqrt(7+helper);
    }
    cout << s;
}
