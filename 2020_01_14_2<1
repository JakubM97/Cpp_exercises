#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    double s = 0.0;

    for(int j = 1; j<=10 ; j++) {
        for(int k = 3; k<=5; k++) {
            s+=j*k;
        }
        for(int c = 1; c<=7; c++) {
            s+=(c+j)/c;
        }
    }
    cout << "d) S = " << s << endl;

    double sl = 0.0;

    for(int n = 1; n<=5; n++) {
        double helper = 0.0;
        for(int i = 0; i<=6; i++){
            helper = n*i;
        }
        sl+=helper;
    }
    cout << "e) Sl = " << sl << endl;

    double ss = 0.0;

    ss+=5;
    for(int j = 1; j<=4; j++){
        double helper = 0.0;
        for(int k=1; k<=7; k++) {
            helper=k/j;
        }

        ss+=sqrt(j)/1+helper;
    }
    cout << "f) S = " << ss << endl;
    
    return 0;
}
